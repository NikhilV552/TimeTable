<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.narendra.timetable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/timetable_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/timetable_app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TimeTable">
        <activity android:name=".Activity.EditTimeTableActivity"/>
        <activity android:name=".Activity.CreateActivity" />
        <activity android:name=".Activity.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity">
        </activity>
    </application>

</manifest>